<%- include('../partials/head.ejs'); %>
<div class="min-h-full">
    <%- include('./components/dashboard_header.ejs'); %>
    <header class="bg-white shadow">
      <div class="mx-auto max-w-7xl px-4 py-6 sm:px-6 lg:px-8 flex justify-between">
        <h1 class="text-3xl font-bold tracking-tight text-gray-900">User</h1>
        <a href="/add-user" class="bg-sky-500 hover:bg-sky-700 px-4 py-2 rounded-full text-white text-sm font-bold">Add User</a>
      </div>
   
    </header>
    <main>
      <div class="mx-auto max-w-7xl px-4 py-6 sm:px-6 lg:px-8">
        <ul role="list" class="divide-y divide-gray-100">
            <% result.forEach(e => {%>
            <li class="flex justify-between gap-x-6 py-5">
              <div class="flex min-w-0 gap-x-4">
                <img class="h-12 w-12 flex-none rounded-full bg-gray-50" src="https://images.unsplash.com/photo-1494790108377-be9c29b29330?ixlib=rb-1.2.1&ixid=eyJhcHBfaWQiOjEyMDd9&auto=format&fit=facearea&facepad=2&w=256&h=256&q=80" alt="">
                <div class="min-w-0 flex-auto">
                  <p class="text-sm font-semibold leading-6 text-gray-900"><%= e.username %></p>
                  <p class="mt-1 truncate text-xs leading-5 text-gray-500"><%= e.email  %></p>
                </div>
              </div>
              <div class="hidden shrink-0 sm:flex sm:flex-col sm:items-end">
                <p class="text-sm leading-6 text-gray-900"><%= e.role_name %></p>
              </div>
            </li>
            <%   });%>
          </ul>
          
      </div>
    </main>
  </div>
  